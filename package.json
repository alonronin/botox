{
	"name": "botox-sdk",
	"version": "1.0.1",
	"private": true,
	"workspaces": [
		"libs/*",
		"examples/*",
		"services/*"
	],
	"devDependencies": {
		"prettier": "^2.1.2"
	},
	"prettier": {
		"useTabs": false,
		"singleQuote": true,
		"trailingComma": "none",
		"printWidth": 100,
		"overrides": [
			{
				"files": "*.json",
				"options": {
					"parser": "json"
				}
			}
		]
	},
	"dependencies": {
		"dotenv": "^8.2.0"
	}
}
